  

<%- include('../partials/user_header') %>



  <section class="head bg-dark" id="tophead" style=" padding-top: 3rem;  padding-bottom: 10px;">
    <div class="container">
      <% if (message) { %>
        <div class="alert alert-dismissible fade show alert-<%= message.type %>" role="alert">
          <button class="btn-close" type="button" data-bs-dismiss="alert" aria-label="Close"></button>
          <strong>
            <%= message.message %>
          </strong>
        </div>
        <% } %>
       
          <div class="row">
            <% banners.forEach((row,index) => { %>
              <% if(row.deleted == false){ %>
            <div class="col-lg-6">
              <h1 class="py-5" style=" color: rgb(255, 255, 255);"> <%= row.name %>
              </h1>
              <p class="py-3" style="color: rgba(139, 142, 153, 1); font-size: medium;"><%= row.description %></p>
              <button class="btn btn-light  mb-3"  ><a href="/categories" class="text-dark" >SHOP NOW</a></button>
            </div>
            <div class="col-lg-6">
              <img src="/uploads/<%= row.image %>" class="img-fluid" alt="homeimg">
            </div>
            <% } %>
            <% }) %>
          </div>
    </div>
  </section>


<section>
<div class="container-fluid my-5">
  <div class="row" id="cathome" style="display: flex; justify-content: center;">
    <% categories.forEach((row,index) => { %>
      <% if(row.deleted == false){ %>
       
    <div class="col-md-4 my-5" id="maincat"  >
      <a href="/categories?category=<%= row._id %>">
      <div>
        <h4 class="text-center text-dark fw-bold pt-5 text-capitalize"> <%= row.name %></h4>
        <p class="text-center text-dark fw-normal"><%= row.description %></p>
      </div>
    
      <img src="/uploads/<%= row.image %>" width="100%">
    </a>
    </div>
  
    <% } %>
    <% }) %>
